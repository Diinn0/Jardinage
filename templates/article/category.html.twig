{% extends 'base.html.twig' %}

{% block title %}Hello ArticleController!{% endblock %}

{% block content %}
    <div class="row mt-5">
        {% for category in categories %}
            <div class="col-md-4 mt-5">
                <a href="{{ path('article', {'id':category.id}) }}">{{ category.label }}</a>

            </div>
        {% endfor %}
    </div>
{% endblock %}